/*
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    Config file for defining DSL2 per module options and publishing paths
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    Available keys to override module options:
        ext.args   = Additional arguments appended to command in module.
        ext.args2  = Second set of arguments appended to command in module (multi-tool modules).
        ext.args3  = Third set of arguments appended to command in module (multi-tool modules).
        ext.prefix = File name prefix for output files.
----------------------------------------------------------------------------------------
*/

process {
    withName: ".*:REGISTRATION_METRICS_TO_ORIG" {
        publishDir = [
            path: { meta.session ? "${params.outdir}/${meta.id}/${meta.session}/dwi/" : "${params.outdir}/${meta.id}/dwi/" },
            mode: params.publish_dir_mode,
                saveAs: {
                    filename ->
                    def prefix = [meta.id, meta.session, meta.run].findAll().join("_")
                    if ( filename.contains("ad") ) { "${prefix}_space-orig_model-tensor_param-ad_dwimap.nii.gz" }
                    else if ( filename.contains("_fa_") ) { "${prefix}_space-orig_model-tensor_param-fa_dwimap.nii.gz" }
                    else if ( filename.contains("_rd_") ) { "${prefix}_space-orig_model-tensor_param-rd_dwimap.nii.gz" }
                    else if ( filename.contains("_md_") ) { "${prefix}_space-orig_model-tensor_param-md_dwimap.nii.gz" }
                    else if ( filename.contains("_mode_") ) { "${prefix}_space-orig_model-tensor_param-mode_dwimap.nii.gz" }
                    else if ( filename.contains("_ga_") ) { "${prefix}_space-orig_model-tensor_param-ga_dwimap.nii.gz" }
                    else if ( filename.contains("_afd_total_") ) { "${prefix}_space-orig_model-csd_param-afdtotal_dwimap.nii.gz" }
                    else if ( filename.contains("_nufo_") ) { "${prefix}_space-orig_model-csd_param-nufo_dwimap.nii.gz" }
                    else if ( filename.contains("_afd_max_") ) { "${prefix}_space-orig_model-csd_param-afdmax_dwimap.nii.gz" }
                    else if ( filename.contains("_afd_sum_") ) { "${prefix}_space-orig_model-csd_param-afdsum_dwimap.nii.gz" }
                    else if ( filename.contains("versions.yml") ) { null }
                }
        ]
    }
    withName: '.*:REGISTRATION_TRACTOGRAM_TO_ORIG' {
        ext.inverse = true
        ext.force = true
        ext.cut_invalid = true
        ext.remove_single_point = true
        ext.remove_overlapping_points = true
        ext.threshold = 0.001
        ext.no_empty = true
        ext.suffix = "warped"
        publishDir = [
            [
                path: { meta.session ? "${params.outdir}/${meta.id}/${meta.session}/dwi/" : "${params.outdir}/${meta.id}/dwi/" },
                mode: params.publish_dir_mode,
                saveAs: {
                    filename ->
                    def prefix = [meta.id, meta.session, meta.run].findAll().join("_")
                    if ( filename.contains("pft_tracking") ) { "${prefix}_space-orig_tract-wholebrain_track-pft_tractogram.trk" }
                    else if ( filename.contains("local_tracking") ) { "${prefix}_space-orig_tract-wholebrain_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("concatenated_tractogram") ) { "${prefix}_space-orig_tract-wholebrain_track-sdstream_desc-ensemble_tractogram.trk" }
                    else if ( filename.contains("commit") ) { "${prefix}_space-orig_tract-wholebrain_seg-DK_desc-filtered_tractogram.h5" }
                    else if ( filename.contains("afd_fixel.h5") ) { "${prefix}_space-orig_tract-wholebrain_seg-DK_desc-preproc_tractogram.h5" }
                    else if ( filename.contains("versions.yml") ) { null }
                }
            ],
            [
                path: { meta.session ? "${params.outdir}/${meta.id}/${meta.session}/dwi/bundles/" : "${params.outdir}/${meta.id}/dwi/bundles/" },
                mode: params.publish_dir_mode,
                saveAs: {
                    filename ->
                    def prefix = [meta.id, meta.session, meta.run].findAll().join("_")
                    if ( filename.contains("AC_labels") ) { "${prefix}_space-orig_tract-AnteriorCommisure_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("AF_L") ) { "${prefix}_space-orig_tract-ArcuateFasciculus_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("AF_R") ) { "${prefix}_space-orig_tract-ArcuateFasciculus_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CC_Fr_1") ) { "${prefix}_space-orig_tract-CorpusCallosumFrontal1_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CC_Fr_2") ) { "${prefix}_space-orig_tract-CorpusCallosumFrontal2_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CC_Oc") ) { "${prefix}_space-orig_tract-CorpusCallosumOccipital_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CC_Pa") ) { "${prefix}_space-orig_tract-CorpusCallosumParietal_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CC_Pr_Po") ) { "${prefix}_space-orig_tract-CorpusCallosumPrecentralPostcentral_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CC_Te") ) { "${prefix}_space-orig_tract-CorpusCallosumTemporal_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CG_L_An") ) { "${prefix}_space-orig_tract-CingulumAnterior_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CG_L_curve") ) { "${prefix}_space-orig_tract-CingulumCurve_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CG_L_Po") ) { "${prefix}_space-orig_tract-CingulumPosterior_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CG_L_labels") ) { "${prefix}_space-orig_tract-Cingulum_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CG_R_An") ) { "${prefix}_space-orig_tract-CingulumAnterior_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CG_R_curve") ) { "${prefix}_space-orig_tract-CingulumCurve_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CG_R_Po") ) { "${prefix}_space-orig_tract-CingulumPosterior_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("CG_R_labels") ) { "${prefix}_space-orig_tract-Cingulum_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("FAT_L") ) { "${prefix}_space-orig_tract-FrontalAslantTract_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("FAT_R") ) { "${prefix}_space-orig_tract-FrontalAslantTract_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("FPT_L_Brainstem") ) { "${prefix}_space-orig_tract-FrontoPontineTractBrainstem_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("FPT_L_labels") ) { "${prefix}_space-orig_tract-FrontoPontineTract_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("FPT_R_Brainstem") ) { "${prefix}_space-orig_tract-FrontoPontineTractBrainstem_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("FPT_R_labels") ) { "${prefix}_space-orig_tract-FrontoPontineTract_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("FX_L") ) { "${prefix}_space-orig_tract-Fornix_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("FX_R") ) { "${prefix}_space-orig_tract-Fornix_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("ICP_L") ) { "${prefix}_space-orig_tract-InferiorCerebellarPeduncle_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("ICP_R") ) { "${prefix}_space-orig_tract-InferiorCerebellarPeduncle_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("IFOF_L") ) { "${prefix}_space-orig_tract-InferiorFrontoOccipitalFasciculus_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("IFOF_R") ) { "${prefix}_space-orig_tract-InferiorFrontoOccipitalFasciculus_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("ILF_L") ) { "${prefix}_space-orig_tract-InferiorLongitudinalFasciculus_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("ILF_R") ) { "${prefix}_space-orig_tract-InferiorLongitudinalFasciculus_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("MCP") ) { "${prefix}_space-orig_tract-MiddleCerebellarPeduncle_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("MdLF_L") ) { "${prefix}_space-orig_tract-MiddleLongitudinalFascicle_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("MdLF_R") ) { "${prefix}_space-orig_tract-MiddleLongitudinalFascicle_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("OR_ML_L") ) { "${prefix}_space-orig_tract-OpticRadiationMeyersLoop_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("OR_ML_R") ) { "${prefix}_space-orig_tract-OpticRadiationMeyersLoop_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("PC_labels") ) { "${prefix}_space-orig_tract-PosteriorCommisure_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("POPT_L_Brainstem") ) { "${prefix}_space-orig_tract-ParietoOccipitoPontineTractBrainstem_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("POPT_R_Brainstem") ) { "${prefix}_space-orig_tract-ParietoOccipitoPontineTractBrainstem_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("POPT_L_labels") ) { "${prefix}_space-orig_tract-ParietoOccipitoPontineTract_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("POPT_R_labels") ) { "${prefix}_space-orig_tract-ParietoOccipitoPontineTract_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("PYT_L_Brainstem") ) { "${prefix}_space-orig_tract-PyramidalTractBrainstem_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("PYT_R_Brainstem") ) { "${prefix}_space-orig_tract-PyramidalTractBrainstem_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("PYT_L") ) { "${prefix}_space-orig_tract-PyramidalTract_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("PYT_R") ) { "${prefix}_space-orig_tract-PyramidalTract_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("SCP_L") ) { "${prefix}_space-orig_tract-SuperiorCerebellarPeduncle_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("SCP_R") ) { "${prefix}_space-orig_tract-SuperiorCerebellarPeduncle_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("SLF_L") ) { "${prefix}_space-orig_tract-SuperiorLongitudinalFasciculus_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("SLF_R") ) { "${prefix}_space-orig_tract-SuperiorLongitudinalFasciculus_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("UF_L") ) { "${prefix}_space-orig_tract-UncinateFasciculus_hemi-left_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("UF_R") ) { "${prefix}_space-orig_tract-UncinateFasciculus_hemi-right_track-sdstream_tractogram.trk" }
                    else if ( filename.contains("versions.yml") ) { null }
                    else { params.clean_output ? null : filename }
                }
            ]
        ]
    }
}
